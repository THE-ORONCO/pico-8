function printTable(tb)
    for k,v in pairs(tb) do
        if (k == "brother") then 
            print("brother:")
            printTable(v)
        else print("  ["..k.."]: "..v) end
    end
end

function modifyBruh(tb)
    tb.bruh = 10
end

function modifyBrother(tb)
    tb.age = 420
end

p = {
    ["bruh"] = 2,
    ["brother"] ={
        ["age"] = 69
    },
    2, 3
}

print("unmodified")
printTable(p)

modifyBruh(p)
modifyBrother(p.brother)

print("modified")
printTable(p)



