-- table definition test
-- THE_ORONCO

function printTable(tb)
    for k,v in pairs(tb) do
        print("  ["..k.."]: "..v)
    end
end

p = {2, 3, 5, 7, 11}

print("normal")
print(#p)               -- should print "5"
printTable(p)

-- add three more elements to the sequence
p[#p+1]=13
p[#p+1]=17
p[#p+1]=19

print("with elements added")
print(#p)               -- should print "8"
printTable(p)

-- shorten the sequence by 2
p[#p]=nil
p[#p]=nil

print("with elements deleted")
print(#p)               -- should print "6"
printTable(p)

-- shorten the sequence by 2 but not from the end
p[#p-3]=nil
p[#p-2]=nil

print("with elements deleted out of order")
print(#p)               -- should print "4"
printTable(p)

